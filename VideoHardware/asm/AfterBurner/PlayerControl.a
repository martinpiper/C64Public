	lda landscapeRotationsFrame
	sec
	sbc #ScreenLandscapeNumRollAnimationSpeed
	bpl .okRollValue2
	clc
	adc #ScreenLandscapeNumRollSteps
.okRollValue2
	sta landscapeRotationsFrame
	jmp .noRollAtAll

.noRollAtAll
}

