!source "stdlib/stdlib.a"
!to "Footy.prg", cbm
!sal
!sl "Footy.map"
!svl "Footy.lbl"
!pdb "Footy.pdb"
!cpu 6510
!ct pet

Mainline_DebugBorder = 1
Mainline_JustSamplesDebugBorder = 1
;Multiplexor_DebugBorder = 1
SampleNMI_DebugBorder = 1

;EnableSamples = 1

!source "stdlib/LongBranches.a"
!source "_f_index1.a"
!source "Scroller/ScrollerMacros.a"
