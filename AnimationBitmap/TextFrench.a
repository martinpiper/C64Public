.text_PanelText1
	!scr "JOURNAL MEMORIEL DU HUNTER.1/THMW1:"
	!by $ff
	!by $ff
	!scr "J'AI ENFIN TROUVE UN TRANSPORT JUSQU'A"
	!by $ff
	!by $ff
	!scr "L'ASTROPORT VELDIN DU SECTEUR D'ORVUS."
	!by $ff
	!by $ff
	!scr "MAIS IMPOSSIBLE DE TRAVERSER LA GALAXIE"
	!by $ff
	!by $ff
	!scr "EN STOP. JE VAIS DEVOIR ME DEBROUILLER."
	!by 0

.text_PanelText2
	!scr "JOURNAL MEMORIEL DU HUNTER.2/THMW1:"
	!by $ff
	!by $ff
	!scr "J'AI REUSSI A REUNIR QUELQUES CREDITS"
	!by $ff
	!by $ff
	!scr "POUR PARTICIPER A UNE PARTIE DE BAKKARA."
	!by $ff
	!by $ff
	!scr "JE PEUX PEUT-ETRE GAGNER ASSEZ POUR"
	!by $ff
	!by $ff
	!scr "ACHETER UN VAISSEAU ET FICHER LE CAMP."
	!by 0

.text_PanelText3
	!scr "JOURNAL MEMORIEL DU HUNTER.3/THMW1:"
	!by $ff
	!by $ff
	!scr "LES DIEUX M'ONT SOURI. J'AI DEGOTE"
	!by $ff
	!by $ff
	!scr "UN VAISSEAU, UN BON VIEUX HUNTER MK. 3S."
	!by $ff
	!by $ff
	!scr "ELLE N'EST PAS TRES JOLIE, MAIS ELLE "
	!by $ff
	!by $ff
	!scr "ME PERMETTRA DE RENTRER A LA MAISON."
	!by 0

.text_PanelText4
	!scr "JOURNAL MEMORIEL DU HUNTER.4/THMW1:"
	!by $ff
	!by $ff
	!scr "QUI L'EUT CRU? CETTE VIEILLE CARCASSE"
	!by $ff
	!by $ff
	!scr "AVAIT ENCORE DE L'ENERGIE DANS SES"
	!by $ff
	!by $ff
	!scr "BATTERIES VETUSTES. LA CHANCE EST"
	!by $ff
	!by $ff
	!scr "PEUT-ETRE ENFIN DE MON COTE. PEUT-ETRE."
	!by 0

.text_PanelText5
	!scr "JOURNAL MEMORIEL DU HUNTER.5/THMW1:"
	!by $ff
	!by $ff
	!scr "ON DIRAIT QUE J'AI PARLE TROP VITE. JE"
	!by $ff
	!by $ff
	!scr "CROIS QUE J'AI ATTIRE L'ATTENTION DES"
	!by $ff
	!by $ff
	!scr "FORCES DE L'ORDRE LOCALES. VOYONS UN PEU"
	!by $ff
	!by $ff
	!scr "CE QUE CE BEBE A DANS LE VENTRE."
	!by 0

.text_PanelText6
	!scr "JOURNAL MEMORIEL DU HUNTER.6/THMW1:"
	!by $ff
	!by $ff
	!scr "JE ME SUIS LAISSE ALLER EN TESTANT LES"
	!by $ff
	!by $ff
	!scr "LIMITES DU HUNTER. JE ME SUIS RETROUVE"
	!by $ff
	!by $ff
	!scr "AU BORD DE L'HORIZON D'UN TROU NOIR."
	!by $ff
	!by $ff
	!scr "J'AI ETE PROJETE JUSQU'ICI... MAIS OU?"
	!by 0


