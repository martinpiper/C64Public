.text_PanelText1
	!scr "HUNTER-LOGBUCH.1/THMW1:"
	!by $ff
	!by $ff
	!scr "ENDLICH MITFAHRGELEGENHEIT ZUM RAUMHAFEN"
	!by $ff
	!by $ff
	!scr "VELDIN IM ORVUS-SEKTOR GEFUNDEN."
	!by $ff
	!by $ff
	!scr "KANN ABER NICHT DURCH DIE GANZE GALAXIE"
	!by $ff
	!by $ff
	!scr "TRAMPEN. ZEIT, AN DIE ARBEIT ZU GEHEN."
	!by 0

.text_PanelText2
	!scr "HUNTER-LOGBUCH.2/THMW1:"
	!by $ff
	!by $ff
	!scr "EIN PAAR CREDITS ZUSAMMEN GEKRATZT, UM"
	!by $ff
	!by $ff
	!scr "ALLES AUF EIN BACCARA-SPIEL ZU SETZEN."
	!by $ff
	!by $ff
	!scr "VIELLEICHT GEWINNE ICH GENUG, UM EIN"
	!by $ff
	!by $ff
	!scr "SCHIFF ZU KAUFEN UND VON HIER ABZUHAUEN."
	!by 0

.text_PanelText3
	!scr "HUNTER-LOGBUCH.3/THMW1:"
	!by $ff
	!by $ff
	!scr "DIE GOTTER MEINTEN ES GUT MIT MIR. HABE"
	!by $ff
	!by $ff
	!scr "EIN SCHIFF GEWONNEN, EINS VON DEN GUTEN,"
	!by $ff
	!by $ff
	!scr "ALTEN HUNTER MK. 3S. NICHT MEHR DAS"
	!by $ff
	!by $ff
	!scr "JUNGSTE, ABER ES ERFULLT SEINEN ZWECK."
	!by 0

.text_PanelText4
	!scr "HUNTER-LOGBUCH.4/THMW1:"
	!by $ff
	!by $ff
	!scr "WER HATTE DAS GEDACHT? DIE ALTE MUHLE "
	!by $ff
	!by $ff
	!scr "HAT TATSACHLICH NOCH ETWAS SAFT IN IHREN"
	!by $ff
	!by $ff
	!scr "ALTEN ENERGIEZELLEN. VIELLEICHT HAB ICH"
	!by $ff
	!by $ff
	!scr "JA DOCH EINMAL GLUCK. VIELLEICHT."
	!by 0

.text_PanelText5
	!scr "HUNTER-LOGBUCH.5/THMW1:"
	!by $ff
	!by $ff
	!scr "ZU FRUH GEFREUT. HABE ANSCHEINEND DIE"
	!by $ff
	!by $ff
	!scr "AUFMERKSAMKEIT DER EINHEIMISCHEN"
	!by $ff
	!by $ff
	!scr "GESETZESHUTER ERREGT. DANN SEHEN"
	!by $ff
	!by $ff
	!scr "WIR MAL, WAS DIE ALTE KISTE SO KANN."
	!by 0

.text_PanelText6
	!scr "HUNTER-LOGBUCH.6/THMW1:"
	!by $ff
	!by $ff
	!scr "HABE ES UBERTRIEBEN, ALS ICH DAS SCHIFF"
	!by $ff
	!by $ff
	!scr "GETESTET HABE. BIN IM EREIGNISHORIZONT"
	!by $ff
	!by $ff
	!scr "EINES SCHWARZEN LOCHS GELANDET."
	!by $ff
	!by $ff
	!scr "ES HAT MICH HIERHER GEBRACHT... ODER?"
	!by 0


.text_PanelOutroText1
	!scr "GUTE ARBEIT HUNTER!"
	!by 0
.text_PanelOutroText2
	!scr "DIE DUNKLE SEITE WARTET..."
	!by 0
.text_PanelOutroText3
	!scr "...WIR SEHEN UNS WIEDER!"
	!by 0


