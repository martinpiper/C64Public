.text_PanelText1
	!scr "REGISTRO DI HUNTER.1/THMW1:"
	!by $ff
	!by $ff
	!scr "FINALMENTE HO RIMEDIATO UN PASSAGGIO PER"
	!by $ff
	!by $ff
	!scr "LO SPAZIOPORTO DI VELDIN, SETTORE ORVUS."
	!by $ff
	!by $ff
	!scr "NON SARA COSI PER TUTTA LA GALASSIA,"
	!by $ff
	!by $ff
	!scr "PERO. BISOGNA SBRIGARSI."
	!by 0

.text_PanelText2
	!scr "REGISTRO DI HUNTER.2/THMW1:"
	!by $ff
	!by $ff
	!scr "HO RACIMOLATO DEI CREDITI PER"
	!by $ff
	!by $ff
	!scr "GIOCARE AL RISCHIOSO BAKKARA."
	!by $ff
	!by $ff
	!scr "CON LA VINCITA COMPREREI UNA NAVICELLA"
	!by $ff
	!by $ff
	!scr "PER ANDARE VIA DA QUESTA ROCCIA."
	!by 0

.text_PanelText3
	!scr "REGISTRO DI HUNTER.3/THMW1:"
	!by $ff
	!by $ff
	!scr "GLI DEI MI SORRIDEVANO. HO VINTO"
	!by $ff
	!by $ff
	!scr "UNA NAVICELLA, UNA DEL BUON VECCHIO"
	!by $ff
	!by $ff
	!scr "HUNTER MK. 3S. NON E GRANCHE, MA "
	!by $ff
	!by $ff
	!scr "MI PORTERA A CASA."
	!by 0

.text_PanelText4
	!scr "REGISTRO DI HUNTER.4/THMW1:"
	!by $ff
	!by $ff
	!scr "COSA NE SAI? LE BATTERIE DELLA"
	!by $ff
	!by $ff
	!scr "VECCHIA RAGAZZA SI CONSERVANO"
	!by $ff
	!by $ff
	!scr "ANCORA BENE. FORSE LA FORTUNA"
	!by $ff
	!by $ff
	!scr "E DALLA MIA PARTE, DOPOTUTTO. FORSE."
	!by 0

.text_PanelText5
	!scr "REGISTRO DI HUNTER.5/THMW1:"
	!by $ff
	!by $ff
	!scr "FORSE HO PARLATO TROPPO PRESTO."
	!by $ff
	!by $ff
	!scr "DEVO AVER ATTIRATO L'ATTENZIONE"
	!by $ff
	!by $ff
	!scr "DELLE FORZE DI POLIZIA LOCALI. E ORA"
	!by $ff
	!by $ff
	!scr "DI METTERE ALLA PROVA QUESTA RAGAZZINA."
	!by 0

.text_PanelText6
	!scr "REGISTRO DI HUNTER.6/THMW1:"
	!by $ff
	!by $ff
	!scr "MI SONO LASCIATO ANDARE METTENDO"
	!by $ff
	!by $ff
	!scr "HUNTER NEL SUO CAMMINO. ALLA FINE HO"
	!by $ff
	!by $ff
	!scr "AGGIRATO UN BUCO NERO, PUNTO DI NON"
	!by $ff
	!by $ff
	!scr "RITORNO. SONO FINITO QUI... MA DOVE?"
	!by 0


